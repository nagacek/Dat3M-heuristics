{
  atomic_t x = ATOMIC_INIT(0);
  atomic_t y = ATOMIC_INIT(0);
}

P0(atomic_t *x, atomic_t *y) {
  int r0; int r1;
  atomic_set(y,1);
  r0 = atomic_fetch_add_relaxed(1,x);
}

P1(atomic_t *x, atomic_t *y) {
  int r0; int r1;
  r0 = atomic_read_acquire(x);
  r1 = atomic_read(y);
}

exists (0:r0 = 0 /\ 1:r0 = 1 /\ 1:r1 = 0)
C C-IRIW+o-o+o-o
(* Allow: No ordering. *)
{
x = 0;
y = 0;
}

P0(int *x, int *y)
{
	int r1; int r2;
	r1 = READ_ONCE(*x);
	r2 = READ_ONCE(*y);
}

P1(int *y, int *x)
{
	int r3; int r4;
	r3 = READ_ONCE(*y);
	r4 = READ_ONCE(*x);
}

P2(int *x)
{
	WRITE_ONCE(*x, 1);
}

P3(int *y)
{
	WRITE_ONCE(*y, 1);
}

exists (0:r1 = 1 /\ 0:r2 = 0 /\ 1:r3 = 1 /\ 1:r4 = 0)

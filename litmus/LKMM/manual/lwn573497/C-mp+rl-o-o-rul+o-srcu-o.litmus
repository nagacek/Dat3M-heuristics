C C-MP+rl-o-o-rul+o-srcu-o
(* Forbid: At least as many GPs and RCU RS CSes. *)
{
a = 1;
x = 2;
}

P0(int *x, int *a)
{
	rcu_read_lock();
	WRITE_ONCE(*a, 3);
	WRITE_ONCE(*x, 4);
	rcu_read_unlock();
}

P1(int *a, int *x)
{
	int r1; int r2;
	r1 = READ_ONCE(*x);
	synchronize_rcu();
	r2 = READ_ONCE(*a);
}

exists (1:r1 = 4 /\ 1:r2 = 1)
